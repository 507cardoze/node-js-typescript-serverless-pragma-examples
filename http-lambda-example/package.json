{
  "name": "http-lambda-example",
  "version": "1.0.0",
  "description": "example",
  "scripts": {
    "lint": "eslint --fix --ext .jsx --ext .js --ext .ts --ext .tsx . --ignore-pattern '**/*.d.ts'",
    "test": "jest --ci --detectOpenHandles --collectCoverage --testPathIgnorePatterns=.build",
    "test:dev": "npm run lint && jest --detectOpenHandles --watchAll --collectCoverage  --testPathIgnorePatterns=.build",
    "offline": "serverless offline start --reloadHandler --stage dev --region us-east-1",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/507cardoze/node-js-typescript-serverless-pragma-examples"
  },
  "author": "Pragma",
  "license": "ISC",
  "engines": {
    "node": "18.18.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.11.24",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "serverless": "3.38.0",
    "serverless-esbuild": "1.51.0",
    "serverless-offline": "13.3.3",
    "serverless-plugin-resource-tagging": "1.2.0",
    "serverless-prune-plugin": "2.0.2",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.525.0",
    "follow-redirects": "1.15.6",
    "serverless-tscpaths": "0.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && npm run lint"
    }
  }
}
